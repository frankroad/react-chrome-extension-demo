{
    "name": "react-chrome-extension-demo",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "react-chrome-extension-demo",
        "default_popup": "popup.html"
    },
    "background": {
        "page": "background.html"
    },
    "permissions": ["storage", "contextMenus"]
}
